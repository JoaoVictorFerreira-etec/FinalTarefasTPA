import java.util.Scanner;

public class clima {
	public static void main (String [] args) {
		Scanner ler = new Scanner (System.in);
		int tempFah, tempCel;
		
		System.out.println ("Insira a temperatura em Fahrenheit (째F): ");
		tempFah = ler.nextInt ();
		
		tempCel = (tempFah-32)*5/9;
	
		if (tempCel<15) {
			System.out.println (tempCel + "째C Frio.");
		} else if (tempCel>=15 && tempCel<22) {
			System.out.println (tempCel + "째C Fresco.");
		} else if (tempCel>=22) { 
			System.out.println (tempCel + "째C Calor");
		}
		ler.close();
	}
}
